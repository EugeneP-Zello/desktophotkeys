{
  "name": "desktophotkeys",
  "version": "1.4.3",
  "description": "This package provides press/release callbacks for system-wide hotkeys on Windows",
  "main": "index.js",
  "gypfile": true,
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "echo \"Test not applicable\""
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EugeneP-Zello/desktophotkeys.git"
  },
  "keywords": [
    "GlobalHotkey"
  ],
  "author": "Zello Inc",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/EugeneP-Zello/desktophotkeys/issues"
  },
  "binary": {
    "module_name": "desktophotkeys",
    "module_path": "./lib/binding/napi-v{napi_build_version}-{platform}-{arch}",
    "host": "https://github.com/EugeneP-Zello/desktophotkeys/releases/download/",
    "remote_path": "{version}",
    "package_name": "napi-v{napi_build_version}-{platform}-{arch}.tar.gz",
    "napi_versions": [
      6
    ]
  },
  "homepage": "https://github.com/zelloptt/desktophotkeys#readme",
  "devDependencies": {
    "np": "^6.5.0"
  },
  "np": {
    "yarn": false,
    "anyBranch": true
  },
  "dependencies": {
    "node-addon-api": "^3.0.2",
    "node-gyp": "7.1.0",
    "node-pre-gyp": "^0.15.0",
    "node-pre-gyp-github": "^1.4.3"
  }
}
